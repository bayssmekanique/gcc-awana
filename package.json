{
  "name": "gcc-awana",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "android:build": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-build-android",
    "android:build:www": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-build-only-www-android",
    "android:build:run": "npm run android:build:www && cd src-cordova && cordova run android",
    "android:dev": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-serve-android",
    "cordova:prepare": "vue-cli-service cordova-prepare",
    "cordova:version": "node ./.scripts/cordovaVersion.js",
    "electron:build": "vue-cli-service electron:build",
    "electron:dev": "vue-cli-service electron:serve",
    "ios:build": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-build-ios",
    "ios:build:cli": "npm run ios:build:www && cd src-cordova && cordova build ios --release --device",
    "ios:build:www": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-build-only-www-ios",
    "ios:build:run": "npm run ios:build:www && cd src-cordova && cordova run ios --device",
    "ios:dev": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-serve-ios",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "web:build": "vue-cli-service build",
    "web:build:run": "npm run web:build && http-server ./dist",
    "web:dev": "vue-cli-service serve"
  },
  "main": "background.js",
  "dependencies": {
